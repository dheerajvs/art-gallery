{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@material/react-button": "^0.3.0",
    "@material/react-card": "^0.3.0",
    "@material/react-material-icon": "^0.3.0",
    "@material/react-top-app-bar": "^0.3.0",
    "gatsby": "2.0.0-beta.36",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-netlify-cms": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "netlify-cms": "^1.9.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path node_modules/ src/ -o src/",
    "develop-css": "npm run build-css && node-sass-chokidar --include-path node_modules/ src/ -o src/ --watch --recursive",
    "build-js": "gatsby build",
    "build": "npm-run-all -p build-css build-js",
    "develop-js": "gatsby develop",
    "develop": "npm-run-all -p develop-css develop-js",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
