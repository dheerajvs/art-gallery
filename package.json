{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Dheeraj Vepakomma <dheerajvs@gmail.com>",
  "dependencies": {
    "@material/react-button": "^0.3.0",
    "@material/react-card": "^0.3.0",
    "@material/react-material-icon": "^0.3.0",
    "@material/react-top-app-bar": "^0.3.0",
    "babel-plugin-styled-components": "^1.5.1",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-netlify-cms": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sass": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-styled-components": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "justified-layout": "^2.1.1",
    "netlify-cms": "^2.0.8",
    "node-sass-chokidar": "^1.3.0",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^3.3.3",
    "styled-components-spacing": "^3.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path node_modules/ src/ -o src/",
    "develop-css": "yarn build-css && node-sass-chokidar --include-path node_modules/ src/ -o src/ --watch --recursive",
    "build-js": "gatsby build",
    "build": "run-s build-css build-js",
    "develop-js": "gatsby develop",
    "develop": "run-p develop-css develop-js",
    "clean": "rimraf public/ .cache/",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dheerajvs/art-gallery"
  }
}
